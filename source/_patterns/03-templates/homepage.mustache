<div class="ah_l ah_l--two-column-fixed">

    <div class="ah_l_secondary">

        {{> organisms-header(headerStyleModifier: "ah_c_header--vertical") }}

    </div><!--end ah_l_secondary-->

    <div class="ah_l_main">

        <div class="ah_l_container">

            <main role="main">

                    {{> molecules-hero }}

                    {{# benefitsSection }}
                    <section class="ah_c_section">

                        {{> molecules-section-header }}

                        <div class="ah_c_text-passage">
                            {{{ htmlText }}}
                        </div>

                    </section>
                    {{/ benefitsSection }}

                    {{# appGallerySection }}
                    <section class="ah_c_section">

                        {{> molecules-section-header }}

                        {{# tileSection}}
                        {{> organisms-tile-list }}
                        {{/ tileSection}}

                    </section>
                    {{/ appGallerySection }}

                    {{# contributeSection }}
                    <section class="ah_c_section">

                        {{> molecules-section-header }}
                        <div class="fpo-block">Discussion/conversation/contributing</div>

                    </section>
                    {{/ contributeSection }}

            </main><!--end role=main-->

            {{> organisms-footer }}

        </div><!--end ah_l_container-->

    </div><!--end ah_l_main-->

</div><!--end ah_l--two-column-fixed-->
